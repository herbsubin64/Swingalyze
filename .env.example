# Swingalyze Environment Variables Template
# Copy this to .env.local for development

# App Configuration
NEXT_PUBLIC_APP_URL=https://your-domain.com

# Supabase Configuration (get from https://supabase.com/dashboard)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe Configuration (get from https://dashboard.stripe.com)
STRIPE_SECRET_KEY=sk_test_your-secret-key
STRIPE_PRICE_ID=price_your-monthly-plan
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Development
NODE_ENV=development
NEXT_PUBLIC_FALLBACK_ENABLED=true
